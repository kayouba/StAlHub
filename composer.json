    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml",
    "phinx:migrate": "vendor/bin/phinx migrate",
    "phinx:rollback": "vendor/bin/phinx rollback"
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
